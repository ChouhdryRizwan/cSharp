@model List<Student>
<h1>Students</h1>
<a asp-controller="Home" asp-action="Create"></a>
<table>
    <thead>
        <th>Name</th>
        <th>Email</th>
        <th>Address</th>
    </thead>

   <tbody>
        @{
            int count = 1;
        }
        @foreach (var std in Model)
        {
            <tr>
                <td>@count</td>
                <td>@std.Name</td>
                <td>@std.Email</td>
                <td>@std.Address</td>
            </tr>
            count++;
        }
    </tbody>
</table>